//Programa que pide al teclado tres números (datos enteros) y sean almacenados en tres variables,llamadas v1, v2 y v3, para después intercambiar sus valores entre sí
#include<stdio.h>

int main(){
	
	int v1,v2,v3;
	
	printf("Escriba v1:\n");
	scanf("%d",&v1);
	printf("Escriba v2:\n");
	scanf("%d",&v2);
	printf("Escriba v3:\n");
	scanf("%d",&v3);

int temp;

	temp=v1;
	v1=v2;
	v2=v3;
	v3=temp;
	
	printf("V1 es:%d\n",v1);
	printf("V2 es:%d\n",v2);
	printf("V3 es:%d\n",v3);
	
	
	return 0;
}
